                 <script>
                    var mndFileds=new Array('First Name','Last Name','Mobile','CONTACTCF1');
                    var fldLangVal=new Array('First Name','Last Name','Mobile','הסניף הקרוב אליך');
                     var name='';
                     var email=''; 
               
             function disableSubmitUpwhileReset()
                 {		
                 var submitbutton = document.getElementById('formsubmitup');
                 if(document.getElementById('privacyToolUp')!=undefined || document.getElementById('consentTool')!=undefined)
                 {
                     submitbutton.setAttribute('disabled', true);
                 submitbutton.setAttribute('style','opacity:0.5;');
              }
                 else
                 {
                     submitbutton.removeAttribute('disabled');
                 }
             } 
             function disableSubmitUp()
                 {		
                 var isprivacyToolAvail = false;
                 var isconsentToolAvail = false;
                 var bothtoolAvail = false;
                 var privacytool='';
                 var consenttool='';
                 var submitbutton = document.getElementById('formsubmitup');
                 if(document.getElementById('privacyToolUp')!=undefined && document.getElementById('consentTool')!=undefined)
                 {
                         isprivacyToolAvail=true;
                         privacytool = document.getElementById('privacyTool');
                         isconsentToolAvail=true;
                         consenttool=document.getElementById('consentTool');
                         bothtoolAvail=true;
                 }
             
                 if(document.getElementById('privacyToolUp')!=undefined)
                 {
                         isprivacyToolAvail=true;
                         privacytool = document.getElementById('privacyToolUp');
                 }
                 if(document.getElementById('consentTool')!=undefined)
                 {
                         isconsentToolAvail=true;
                         consenttool=document.getElementById('consentTool');
                 }
                 if(submitbutton.hasAttribute('disabled') && bothtoolAvail && consenttool.checked && privacytool.checked)
                 {
                         submitbutton.removeAttribute('disabled');
                 submitbutton.removeAttribute('style','opacity: none;');}
                 else if(submitbutton.hasAttribute('disabled') && isprivacyToolAvail && privacytool.checked && consenttool=='')
                 {
                         submitbutton.removeAttribute('disabled');
                 submitbutton.removeAttribute('style','opacity: none;');}
                 else if(submitbutton.hasAttribute('disabled') && isconsentToolAvail && consenttool.checked && privacytool=='')
                 {
                         submitbutton.removeAttribute('disabled');
             submitbutton.removeAttribute('style','opacity: none;');
                 }
                 else
                 {
                         submitbutton.setAttribute('disabled', true);
                 submitbutton.setAttribute('style','opacity: 0.5;');}
             }
             
                    function checkMandatory3169284000013334005() {
                     for(i=0;i<mndFileds.length;i++) {
                       var fieldObj=document.forms['WebToContacts3169284000013334005'][mndFileds[i]];
                       if(fieldObj) {
                         if (((fieldObj.value).replace(/^\s+|\s+$/g, '')).length==0) {
                          if(fieldObj.type =='file')
                             { 
                              alert('Please select a file to upload.'); 
                              fieldObj.focus(); 
                              return false;
                             } 
                         alert(fldLangVal[i] +' cannot be empty.'); 
                                   fieldObj.focus();
                                   return false;
                         }  else if(fieldObj.nodeName=='SELECT') {
                                  if(fieldObj.options[fieldObj.selectedIndex].value=='-None-') {
                             alert(fldLangVal[i] +' cannot be none.'); 
                             fieldObj.focus();
                             return false;
                            }
                         } else if(fieldObj.type =='checkbox'){
                             if(fieldObj.checked == false){
                             alert('Please accept  '+fldLangVal[i]);
                             fieldObj.focus();
                             return false;
                            } 
                          } 
                          try {
                              if(fieldObj.name == 'Last Name') {
                             name = fieldObj.value;
                                }
                         } catch (e) {}
                         }
                     }
                      }
                    
             </script>
